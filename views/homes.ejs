<div class="row">
    <% homes.forEach(function(home) { %>
        <div class="col-sm-6">
            <div class="card" style="width: 18rem;">
                <img src="/images/<%= home.type %>.jpg" class="card-img-top" alt="..." height="200">
                <div class="card-body">
                <h5 class="card-title"><%= home.type %></h5>
                <p class="card-text"><%= home.description %></p>
                </div>
                <ul class="list-group list-group-flush">
                <li class="list-group-item">Tarifa corrente: <%= home.rate %></li>
                <li class="list-group-item">CÃ´modos:</li>
                <li class="list-group-item">Consumo estimado: <%= home.consumption %></li>
                <li class="list-group-item">Custo Mensal: <%= home.monthlyCost %></li>
                </ul>
                <div class="card-body">
                <a href="#" class="card-link">Editar</a>
                <a href="#" class="card-link">Excluir</a>
                </div>
            </div>
        </div>
    <% }); %>
</div>